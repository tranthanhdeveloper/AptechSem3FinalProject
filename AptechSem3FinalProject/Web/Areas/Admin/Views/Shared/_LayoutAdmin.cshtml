
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/jvectormap/jquery-jvectormap.css">
    @*<link rel="stylesheet" href="~/Content/Instructor/dist/css/AdminLTE.min.css">*@
    <link href="~/Content/Admin/AdminLTE.css" rel="stylesheet" />
    <link href="~/Content/Admin/style.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/Instructor/dist/css/skins/_all-skins.min.css">
    <link href="~/Content/bootstrap-dialog.css" rel="stylesheet" />
    <link href="~/Content/Instructor/authorsite.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-ui.css" rel="stylesheet" />
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="skin-green skin-cocobase sidebar-mini">
    <div class="wrapper">

        @Html.Partial("_LayoutAdminHeader");
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar" style="height: auto;">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="dist/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>Alexander Pierce</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>

                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu tree" data-widget="tree">
                    <li class="header">MAIN NAVIGATION</li>

                    <li class="active">
                        <a href="@Url.Action("ManageUser","User")">
                            <i class="fa fa-user"></i>
                            <span>User</span>
                            
                        </a>
                        
                    </li>
                    
                    <li class="active">
                        <a href="widgets.html">
                            <i class="fa fa-th"></i> <span>ManageAll</span>
                            
                        </a>
                    </li>

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <div class="content-wrapper" style="height: 100%">
            @RenderBody()
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.4.0
            </div>
            <strong>Copyright © 2018 </strong>

        </footer>
        <div class="control-sidebar-bg"></div>
    </div>
    <script src="~/Content/Instructor/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/Content/Instructor/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Content/Instructor/bower_components/fastclick/lib/fastclick.js"></script>
    <script src="~/Content/Instructor/bower_components/select2/dist/js/select2.full.min.js"></script>
    <script src="~/Content/Instructor/dist/js/adminlte.min.js"></script>
    <script src="~/Content/Instructor/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
    <script src="~/Content/Instructor/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/Instructor/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/Instructor/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Content/Instructor/dist/js/pages/dashboard2.js"></script>
    <script src="~/Content/Instructor/dist/js/demo.js"></script>
    <script src="~/Scripts/Instructor/author.js"></script>
    <script src="~/Scripts/Admin/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/js/jquery-ui.js"></script>
    <script src="~/Scripts/Admin/Form.js"></script>
    <script src="~/Scripts/Admin/layout.js"></script>
    <script src="~/Scripts/Admin/ManageDataTable.js"></script>
    <script src="~/Scripts/Admin/bootstrap-dialog.js"></script>
    <script src="~/Scripts/Admin/chartjs/Chart.js"></script>
    @RenderSection(@"Scripts", false)
</body>
</html>