    @*<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">*@
    <style>
        @@import url(https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic);
    </style>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Admin</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/Ionicons/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/bower_components/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/Content/Instructor/dist/css/AdminLTE.min.css">
    <link rel="stylesheet" href="~/Content/Instructor/dist/css/skins/_all-skins.min.css">
    <link href="~/Content/Instructor/authorsite.css" rel="stylesheet" />
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">

    @if (IsSectionDefined(@"Styles"))
    {
        @RenderSection(@"Styles", false)
    }
</head>
<body class="hold-transition login-page">
<div class="login-box">
    <div class="login-logo">
        Login
    </div>
    <!-- /.login-logo -->
    <div class="login-box-body">
        @RenderBody()
    </div>
</div>
<!-- /.login-box -->
<script src="~/Content/Instructor/bower_components/jquery/dist/jquery.min.js"></script>
<script src="~/Content/Instructor/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="~/Content/Instructor/bower_components/fastclick/lib/fastclick.js"></script>
<script src="~/Content/Instructor/bower_components/select2/dist/js/select2.full.min.js"></script>
<script src="~/Content/Instructor/dist/js/adminlte.min.js"></script>
<script src="~/Content/Instructor/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
<script src="~/Content/Instructor/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
<script src="~/Content/Instructor/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
<script src="~/Content/Instructor/bower_components/jquery-slimscroll/jquery.slimscroll.min.js"></script>
<script src="~/Content/Instructor/dist/js/pages/dashboard2.js"></script>
<script src="~/Content/Instructor/dist/js/demo.js"></script>
<script src="~/Scripts/Instructor/author.js"></script>
